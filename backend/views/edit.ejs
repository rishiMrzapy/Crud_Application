<!DOCTYPE html>
<html lang="en">
<head>
    <title>Document</title>

    <link rel= "stylesheet" href= "https //maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.csslink rel= stylesheet href= https //maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">

</head>
<body>
    <div class="container">
        <% users.forEach(function(user){ %>

        <form method="post" action="/update/<%=user.users_id %>" autocomplete="off"> 
            <div class="form-group">
                <label><b>User name:</b></label>
                <input type="text" name="name" class="form-control" value="<%= user.user_name %>">
            </div>
            <div class="form-group">
                <label><b>User Email:</b></label>
                <input type="email" name="email" class="form-control" value="<%= user.user_email %>">
            </div>
            <div class="form-group">
                <label><b>User Password:</b></label>
                <input type="password" name="password" class="form-control" value="<%= user.user_password %>">
            </div>
            <input type="submit" name="update" value="Update" class="btn btn-primary">
        </form>
        <% })%>

    </div>
    
</body>
</html>